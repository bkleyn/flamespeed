<?xml version="1.0"?>

<ctml>

    <phase>
        <speciesArray> N N2 H NH NH2</speciesArray>
    </phase>

    <reactionData id="test_mechanism">

        <!-- reaction 01  -->
        <reaction reversible="no" type="Elementary" id="reaction01">
            <equation>N + NH =] N2 + H</equation>
            <rateCoeff>
                <Arrhenius>
                    <A>3.52e+10</A>
                    <E>7.14e+04</E>
                </Arrhenius>
            </rateCoeff>
            <reactants>N:1 NH:1</reactants>
            <products>N2:1 H:1</products>
        </reaction>

        <!-- reaction 02 -->
        <reaction reversible="no" type="Elementary" id="reaction02">
            <equation>2N + 2H =] NH2 + N</equation>
            <rateCoeff>
                <modifiedArrhenius>
                    <A>5.06e-2</A>
                    <b>2.7</b>
                    <E>2.63e+04</E>
                </modifiedArrhenius>
            </rateCoeff>
            <reactants>N:2 H:2</reactants>
            <products>NH2:1 N:1</products>
        </reaction>

    </reactionData>

</ctml>
